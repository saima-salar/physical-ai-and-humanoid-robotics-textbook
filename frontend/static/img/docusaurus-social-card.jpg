<svg width="1200" height="630" viewBox="0 0 1200 630" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#F0F9FF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#EDE9FE;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="ai-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4F46E5;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#7C3AED;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur in="SourceAlpha" stdDeviation="8"/>
      <feOffset dx="6" dy="6" result="offsetblur"/>
      <feFlood flood-color="rgba(0,0,0,0.15)"/>
      <feComposite in2="offsetblur" operator="in"/>
      <feMerge>
        <feMergeNode/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <rect width="100%" height="100%" fill="url(#bg-gradient)"/>
  
  <!-- Decorative elements -->
  <g opacity="0.2">
    <path d="M200,400 L300,300 L400,400 L500,200 L600,350 L700,150 L800,300 L900,250 L1000,400" 
          stroke="#4F46E5" stroke-width="4" fill="none"/>
    <circle cx="300" cy="150" r="20" fill="#4F46E5"/>
    <circle cx="600" cy="100" r="15" fill="#7C3AED"/>
    <circle cx="900" cy="200" r="25" fill="#4F46E5"/>
  </g>
  
  <!-- Central humanoid robot -->
  <g transform="translate(500, 250) scale(2)">
    <!-- Robot head with AI brain -->
    <circle cx="30" cy="20" r="20" fill="url(#ai-gradient)" filter="url(#shadow)"/>
    <!-- AI brain pattern -->
    <g stroke="#FFFFFF" stroke-width="2" fill="none">
      <circle cx="30" cy="20" r="12"/>
      <path d="M24,16 C25,14 27,14 28,16 C29,18 28,20 26,20"/>
      <path d="M34,20 C36,18 38,18 39,20 C40,22 38,24 36,24"/>
    </g>
    <g stroke="#FFFFFF" stroke-width="1" fill="none">
      <line x1="25" y1="18" x2="23" y2="22"/>
      <line x1="35" y1="18" x2="37" y2="22"/>
      <line x1="28" y1="22" x2="32" y2="22"/>
    </g>
    
    <!-- Robot body -->
    <rect x="15" y="40" width="30" height="50" rx="6" fill="url(#ai-gradient)" filter="url(#shadow)"/>
    
    <!-- Arms -->
    <rect x="0" y="50" width="15" height="8" rx="4" fill="url(#ai-gradient)"/>
    <rect x="45" y="50" width="15" height="8" rx="4" fill="url(#ai-gradient)"/>
    
    <!-- Legs -->
    <rect x="20" y="90" width="10" height="25" rx="3" fill="url(#ai-gradient)"/>
    <rect x="30" y="90" width="10" height="25" rx="3" fill="url(#ai-gradient)"/>
  </g>
  
  <!-- Title text -->
  <g transform="translate(450, 450)">
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="48" font-weight="bold" fill="#1E293B">
      Physical AI &amp; Humanoid Robotics
    </text>
    <text x="0" y="40" font-family="Arial, sans-serif" font-size="24" fill="#64748B">
      Mastering the Convergence of AI and Physical Intelligence
    </text>
  </g>
  
  <!-- Bottom tagline -->
  <g transform="translate(500, 580)">
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="18" fill="#4F46E5" text-anchor="middle">
      Embodied Intelligence | ROS 2 | NVIDIA Isaac | Vision-Language-Action
    </text>
  </g>
</svg>